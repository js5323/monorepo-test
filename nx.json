{
  "targetDefaults": {
    "test": {
      "dependsOn": []
    },
    "dev": {
      "cache": true,
      "dependsOn": [
        "^dev"
      ]
    },
    "build": {
      "dependsOn": []
    },
    "lint": {
      "dependsOn": []
    },
    "preview": {
      "dependsOn": []
    }
  },
  "plugins": [
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "vite:build",
        "testTargetName": "test",
        "serveTargetName": "serve",
        "previewTargetName": "vite:preview",
        "serveStaticTargetName": "serve-static"
      }
    }
  ]
}
